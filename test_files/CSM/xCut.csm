# cutwing

# default design parameters
despmtr   series    8412      mptt
despmtr   area      40        len^2
despmtr   aspect    6.0       -
despmtr   taper     0.5       -
despmtr   twist     5.0       deg
despmtr   lesweep   30.0      deg
despmtr   dihedral  5.0       deg
despmtr   fracle    0.0     percent
despmtr   fracte    1.     percent

# internal parameters
set       cmean     sqrt(area/aspect)
set       span      cmean*aspect
set       sspan     span/2
set       croot     2*cmean/(1+taper)
set       ctip      croot*taper
set       xtip      sspan*tand(lesweep)
set       ytip      sspan*tand(dihedral)

set       ybot      -0.1*croot
set       ytop      ytip+0.2*croot+ytip
set       extend    0.00 
set       cutLength 0.15
# wing OML
mark
udprim              naca      Series    series
scale               croot
udprim              naca      Series    series
scale               ctip
rotatez             -twist    0         0
translate           xtip      ytip      -sspan
blend

# cut box
skbeg           -croot          ybot  0
    linseg      -croot          ybot  -sspan*cutLength
    linseg      xtip+croot ybot  -sspan*cutLength
    linseg      xtip+croot ybot  0
    linseg      -croot          ybot  0
skend

extrude         0  ytop-ybot  0

# common regions
intersect

# write the file
dump cutwing.egads

end

